			<audio src="<%= typeof audio_dir_and_name != 'undefined' ? audio_dir_and_name : ''%>" controls autoplay type="audio/mpeg">
				
            </audio>
            <section id="media"></section>
            <br>

	 
		

	<script type="text/javascript">
		
		function myFunction() {
			let input = document.getElementById("voiceIn").value;
			let voice = new p5.Speech();
			voice.speak(input);
		}
	</script>

	  

	    <div class="container">
